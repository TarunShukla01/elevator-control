{
  "compilerOptions": {
  "target": "ES2021",
  "useDefineForClassFields": true,
  "lib": ["DOM", "ES2021"],
  "jsx": "react-jsx",
  "module": "ESNext",
  "moduleResolution": "bundler",
  "strict": true,
  "esModuleInterop": true,
  "skipLibCheck": true,
  "forceConsistentCasingInFileNames": true
  },
  "include": ["src"]
}